<body ng-app="playBackRealTime" ng-controller="playBackRealTimeCtrl">
	<div id="page-inner">
		<div class="row">
			<div class="search-condition"  style="margin-left: 0px;">
				<div class="panel  panel-bottom">
					<!-- <div class="panel-heading panel-heading-weight">实时直播监控图表 </div> -->
					<div class="panel-body">

						<form class="form-inline">
							<div class="row">
								<div  class="search-condition-div chart-condition" ng-show="isShowPeriod">

									<label for="exampleInputName2">监控时段：</label>
									
						           <select   
										class="form-control form-width-90"  ng-model="selMin">
										<option ng-repeat="time in period" value="{{time.min}}" >{{time.mins}}</option>
									</select>
									
									<button class="btn btn-info chart-condition-space" ng-click="sendSearchRequest(0,0)">
										 开始监控
									</button>
									<button class="btn btn-danger" ng-click="sendSearchRequest(0,1)">
										 停止监控
									</button>
								</div>


							<!-- 	<div id="searchBtnDiv" class="search-button chart-condition-space" ">
									
								</div> -->
							</div>
							</form>
							
						
							
							
							
							
							
							
							
							
							
							
							
							
					</div>
				</div>
			</div>
			
			
				
							
							<div  class="chart-left">
							     
							      
							        <div class="chart-left-area">选择地域：</div>
							   	<div  class="chart-left-areas" style="height:180px">
							     <span ng-repeat="area in areas">								 
								<div class="radio">
								  <label><input type="radio" name="opArea" id="opArea" value="{{area.areaCode}}" ng-click="selectArea(area.areaCode)" ng-model="theArea" ng-checked="selectedarea(area.areaCode)">
								  {{ area.areaName }}</label>
								  </div>
								</span>

							      </div>
							
							 
							     <div class="chart-left-area"> 选择频道：</div>
							     <div  class="chart-left-areas" style="height:180px">
								 <div class="checkbox" >
								 <span ng-repeat="channel in channels">
								  <label><input type="checkbox" ng-click="channelDoCheck()" name="channelBox" value="{{channel.serviceId}}" ng-checked="isSelected(channel.serviceId)">
								  {{ channel.channelName }}</label>
								  </span>
								  </div>
								
								</div>
								
								<div class="chart-left-area"> 选择频道组：</div>
							     <div  class="chart-left-areas" style="height:180px">
								 <div class="checkbox" >
								   <span ng-repeat="channelgroup in channelgroups">
								  <label><input type="checkbox" ng-click="channelGroupDoCheck()" name="channelGroupBox" value="{{channelgroup.subServiceIds}}" >
								  {{ channelgroup.channelName }}</label>
								  </span>
								  </div>
								
								</div>
								
								
								
							</div>
							<div  class="chart-right">
							        
							        





